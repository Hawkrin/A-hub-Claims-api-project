@ASP.Claims.API_HostAddress = http://localhost:5020
@claimId = d7c9eb85-5c14-43a1-9d05-56a662ddd3e1

### Get all claims
GET {{ASP.Claims.API_HostAddress}}/api/claim
Accept: application/json


######################### PROPERTY CLAIMS ENDPOINTS ###########################

### Get all property claims
GET {{ASP.Claims.API_HostAddress}}/api/propertyclaim

### Get property claim by id
GET {{ASP.Claims.API_HostAddress}}/api/propertyclaim/{{claimId}}

### Create property claim
POST {{ASP.Claims.API_HostAddress}}/api/propertyclaim
Content-Type: application/json
{
  "address": "123 Main St",
  "description": "Fire damage",
  "estimatedDamageCost": 5000,
  "PropertyDamageType": "Fire",
}

### Update property claim
PUT {{ASP.Claims.API_HostAddress}}/api/propertyclaim/{{claimId}}
Content-Type: application/json
{
  "id": "{{propertyClaimId}}",
  "address": "123 Main St",
  "description": "Updated description",
  "estimatedDamageCost": 6000,
  "propertyDamageType": "Water"
}

### Delete property claim
DELETE {{ASP.Claims.API_HostAddress}}/api/propertyclaim/{{claimId}}

###

######################### VEHICLE CLAIMS ENDPOINTS ###########################

### Get all vehicle claims
GET {{ASP.Claims.API_HostAddress}}/api/vehicleclaim

### Get vehicle claim by id
GET {{ASP.Claims.API_HostAddress}}/api/vehicleclaim/{{claimId}}

### Create vehicle claim
POST {{ASP.Claims.API_HostAddress}}/api/vehicleclaim
Content-Type: application/json
{
  "registrationNumber": "ABC123",
  "placeOfAccident": "Main Street",
  "reportedDate": "2024-06-01T00:00:00Z",
  "description": "Rear-end collision"
}

### Update vehicle claim
PUT {{ASP.Claims.API_HostAddress}}/api/vehicleclaim/{{claimId}}
Content-Type: application/json
{
  "id": "{{claimId}}",
  "registrationNumber": "ABC123",
  "placeOfAccident": "Main Street",
  "type": "Vehicle",
  "reportedDate": "2024-06-02T00:00:00Z",
  "description": "Updated: rear-end collision, more damage found"
}

### Delete vehicle claim
DELETE {{ASP.Claims.API_HostAddress}}/api/vehicleclaim/{{claimId}}

###

######################### TRAVEL CLAIMS ENDPOINTS ###########################

### Get all travel claims
GET {{ASP.Claims.API_HostAddress}}/api/travelclaim

### Get travel claim by id
GET {{ASP.Claims.API_HostAddress}}/api/travelclaim/{{claimId}}

### Create travel claim
POST {{ASP.Claims.API_HostAddress}}/api/travelclaim
Content-Type: application/json
{
  "country": "Spain",
  "startDate": "2024-06-10T00:00:00Z",
  "endDate": "2024-06-15T00:00:00Z",
  "incidentType": "Other",
  "reportedDate": "2024-06-10T00:00:00Z",
  "description": "Lost luggage at airport"
}

### Update travel claim
PUT {{ASP.Claims.API_HostAddress}}/api/travelclaim/{{claimId}}
Content-Type: application/json
{
  "id": "{{claimId}}",
  "country": "Spain",
  "startDate": "2024-06-10T00:00:00Z",
  "endDate": "2024-06-16T00:00:00Z",
  "incidentType": "Other",
  "reportedDate": "2024-06-10T00:00:00Z",
  "description": "Updated: lost luggage, compensation requested"
}

### Delete property claim
DELETE {{ASP.Claims.API_HostAddress}}/api/travelclaim/{{claimId}}

